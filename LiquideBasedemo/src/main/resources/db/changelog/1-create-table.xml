<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">


    <changeSet id="1" author="dinesh">
        <createTable tableName="Student">
            <column name="id" type="int" autoIncrement="true" >
                <constraints primaryKey="true"></constraints>
            </column>
            <column name="name" type="varchar(60)" >

            </column>
            <column name="email" type="varchar(250)" ></column>

            <column name="adr" type="varchar(30)"></column>

        </createTable>

    </changeSet>
    <changeSet id="2" author="dinesh">
        <createTable tableName="teacher">

            <column name="id" type="int" autoIncrement="true"></column>
            <column name="name" type="varchar(40)"></column>
        </createTable>
    </changeSet>
   </databaseChangeLog>